{
    "manifest_version": 3,
    "name": "Discord Bot Chegg",
    "version": "1.0",
    "description": "Bot Buat Discord Chegg",
    "permissions": [
      "tabs",
      "webNavigation",
      "downloads",
      "contextMenus",
      "scripting",
      "notifications",
      "storage",
      "windows",
      "activeTab"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    },
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "content_scripts": [
    {
        "matches": ["https://discord.com/*"],
        "js": ["content_script.js"],
        "run_at": "document_end"
    }
    ],
    "commands": {
      "_execute_action": {
        "suggested_key": {
          "default": "Ctrl+Shift+F"
        },
        "description": "Run findLastElement function"
      }
    }
  }
  